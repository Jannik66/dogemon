<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Endscreen">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="500.0">
         <top>
            <Pane prefHeight="250.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fx:id="WinnerPicture" fitHeight="200.0" fitWidth="200.0" layoutX="150.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true" />
               </children></Pane>
         </top>
         <center>
            <Label fx:id="WinnerName" alignment="CENTER" contentDisplay="CENTER" text="Someone wins!" textAlignment="CENTER" BorderPane.alignment="BOTTOM_CENTER">
               <font>
                  <Font name="Pixel UniCode Regular" size="36.0" />
               </font>
               <BorderPane.margin>
                  <Insets bottom="20.0" />
               </BorderPane.margin>
            </Label>
         </center>
         <bottom>
            <Button mnemonicParsing="false" onAction="#PlayAgain" text="Play Again" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Pixel UniCode Regular" size="25.0" />
               </font>
               <BorderPane.margin>
                  <Insets bottom="30.0" />
               </BorderPane.margin>
            </Button>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
